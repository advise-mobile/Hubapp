{
	"name": "HubApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "npx react-native run-android",
		"ios": "react-native run-ios",
		"android:build": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
		"android:test": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
		"android:debug": "cd android && ./gradlew clean && ./gradlew assembleDebug && cd ..",
		"android:install-debug": "cd android && ./gradlew assembleDebug && adb install -r app/build/outputs/apk/debug/app-debug.apk",
		"start": "react-native start --reset-cache",
		"test": "jest",
		"lint": "eslint .",
		"logcat": "adb logcat *:E",
		"logcat-full": "adb logcat",
		"device-list": "adb devices",
		"test-device": "adb devices | grep -w 'device' | awk '{print $1}' | xargs -I {} adb -s {} shell getprop ro.product.model && yarn android:test",
		"android:install": "adb install android/app/build/outputs/apk/release/app-release.apk",
		"android:clean": "cd android && rm -rf ~/.gradle/caches/ .gradle build && cd ../",
		"android:build-install": "yarn android:clean && cd android && ./gradlew clean && ./gradlew assembleRelease --no-daemon --max-workers 2 && adb install -r -d app/build/outputs/apk/release/app-release.apk && cd ../",
		"android:install-release": "cd android && ./gradlew assembleRelease && adb install -r app/build/outputs/apk/release/app-release.apk",
		"android:release": "cd android && ./gradlew clean && cd .. && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle && cd android && ./gradlew assembleRelease && android:install &&  cd ..",
		"ios:initialize": "yarn install && cd ios && pod install && cd ../ && yarn onesignal",
		"android:initialize": "yarn install && yarn add react-native-onesignal@4.5.4 && yarn android:clean && cp ../Android/hubapp-key.keystore android/app/ &&  yarn android:release",
		"ios:clean": "cd ios && xcodebuild clean && cd ..",
		"ios:clean-full": "cd ios && rm -rf build Pods Podfile.lock && pod deintegrate && pod cache clean --all && pod install && cd ..",
		"ios:full-reset": "rm -rf ~/Library/Developer/Xcode/DerivedData && yarn ios:clean-full && yarn ios:xcode",
		"onesignal": "yarn add react-native-onesignal@4.5.4 && yarn ios:full-reset",
		"ios:xcode": "open ios/HubApp.xcworkspace"
	},
	"dependencies": {
		"@expo/match-media": "^0.4.0",
		"@react-native-async-storage/async-storage": "1.15.17",
		"@react-native-community/checkbox": "^0.5.6",
		"@react-native-community/cli": "^4.13.0",
		"@react-native-community/clipboard": "^1.5.1",
		"@react-native-community/datetimepicker": "^3.0.9",
		"@react-native-community/masked-view": "^0.1.10",
		"@react-native-community/viewpager": "^4.2.1",
		"@react-native-picker/picker": "^1.9.4",
		"@react-navigation/bottom-tabs": "^5.11.9",
		"@react-navigation/native": "^5.9.4",
		"@react-navigation/stack": "^5.14.4",
		"@redux-saga/is": "^1.1.2",
		"axios": "^0.21.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"css-to-react-native": "^3.0.0",
		"jwt-decode": "^3.1.2",
		"moment": "^2.29.1",
		"react": "16.13.1",
		"react-hook-form": "^6.13.1",
		"react-native": "0.63.3",
		"react-native-app-intro-slider": "^4.0.4",
		"react-native-calendars": "^1.1129.0",
		"react-native-datepicker": "^1.7.2",
		"react-native-gesture-handler": "^1.9.0",
		"react-native-image-picker": "3.1.1",
		"react-native-modal": "^11.10.0",
		"react-native-modal-datetime-picker": "^9.1.0",
		"react-native-modalize": "^2.0.8",
		"react-native-onesignal": "4.5.4",
		"react-native-picker-select": "^8.0.4",
		"react-native-popup-menu": "^0.16.1",
		"react-native-reanimated": "^1.13.2",
		"react-native-remote-svg": "^2.0.6",
		"react-native-render-html": "^6.3.4",
		"react-native-responsive-query": "^0.2.2",
		"react-native-safe-area-context": "^3.1.9",
		"react-native-screens": "^2.15.0",
		"react-native-scrollable-tab-view": "^1.0.0",
		"react-native-share-pdf": "^0.0.7",
		"react-native-simple-radio-button": "^2.7.4",
		"react-native-simple-toast": "^1.1.3",
		"react-native-splash-screen": "^3.2.0",
		"react-native-swipe-list-view": "^3.2.5",
		"react-native-tab-view": "^2.15.2",
		"react-native-vector-icons": "^7.1.0",
		"react-native-webview": "^11.17.2",
		"react-navigation": "^4.4.3",
		"react-navigation-stack": "^2.10.1",
		"react-navigation-tabs": "^2.10.1",
		"react-redux": "^7.2.2",
		"react-responsive": "^9.0.2",
		"reactotron-react-native": "^5.0.0",
		"reactotron-redux": "^3.1.3",
		"reactotron-redux-saga": "^4.2.3",
		"redux-saga": "^1.1.3",
		"reduxsauce": "^1.2.0",
		"rn-fetch-blob": "^0.12.0",
		"seamless-immutable": "^7.1.4",
		"styled-components": "^5.2.1",
		"use-debounced-effect": "^0.0.3",
		"vanilla-masker": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/runtime": "^7.8.4",
		"@react-native-community/eslint-config": "^1.1.0",
		"@types/react": "^18.2.8",
		"@types/styled-components": "^5.1.26",
		"babel-jest": "^25.1.0",
		"eslint": "^6.5.1",
		"jest": "^25.1.0",
		"jetifier": "^2.0.0",
		"metro-react-native-babel-preset": "^0.59.0",
		"react-test-renderer": "16.13.1",
		"typescript": "^5.1.3"
	},
	"jest": {
		"preset": "react-native"
	}
}
