{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "jsx": "react",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "baseUrl": ".",
      "paths": {
        "@services/*": ["src/services/*"],
        "@pages/*": ["src/pages/*"],
        "@helpers/*": ["src/helpers/*"],
        "@components/*": ["src/components/*"],
      }
    },
    "include": [
      "./src/**/*"
    ]
  }