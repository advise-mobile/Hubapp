{
	"name": "HubApp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"android": "npx react-native run-android",
		"ios": "react-native run-ios",
		"start": "react-native start --reset-cache",
		"test": "jest",
		"lint": "eslint .",
		"logcat": "adb logcat *:E",
		"logcat-full": "adb logcat",
		"device-list": "adb devices",
		"test-device": "adb devices | grep -w 'device' | awk '{print $1}' | xargs -I {} adb -s {} shell getprop ro.product.model && yarn android:test",
		"fix-boost": "sed -i '' 's|spec\\.source = { :http => '\\''https://boostorg\\.jfrog\\.io/artifactory/main/release/1\\.76\\.0/source/boost_1_76_0\\.tar\\.bz2'\\''|spec.source = { :http => '\\''https://downloads.sourceforge.net/project/boost/boost/1.76.0/boost_1_76_0.tar.bz2'\\''|g' node_modules/react-native/third-party-podspecs/boost.podspec && echo 'ðŸ”§ Boost URL fixed!'",
		"android:gradlew-clean": "cd android && ./gradlew clean && cd ..",
		"android:clean-cache": "cd android && rm -rf ~/.gradle/caches/ .gradle build && cd ..",
		"android:copy-keystore": "cp ../Android/hubapp-key.keystore android/app/",
		"android:bundle-js": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
		"android:onesignal-setup": "yarn add react-native-onesignal@4.5.4",
		"android:build-debug": "cd android && ./gradlew assembleDebug && cd ..",
		"android:build-release": "cd android && ./gradlew assembleRelease && cd ..",
		"android:build-aab": "cd android && ./gradlew bundleRelease && cd ..",
		"android:install-apk": "adb install -r android/app/build/outputs/apk/release/app-release.apk",
		"android:install-debug-apk": "adb install -r android/app/build/outputs/apk/debug/app-debug.apk",
		"android:install-optimized": "adb install -r -d android/app/build/outputs/apk/release/app-release.apk",
		"android:clean": "yarn android:clean-cache && yarn android:gradlew-clean",
		"android:debug": "yarn android:clean && yarn android:build-debug",
		"android:test": "yarn android:clean && yarn android:build-release",
		"android:build": "yarn android:clean && yarn android:build-aab",
		"android:install-debug": "yarn android:build-debug && yarn android:install-debug-apk",
		"android:install-release": "yarn android:build-release && yarn android:install-apk",
		"android:build-install": "yarn android:clean && yarn android:build-release --no-daemon --max-workers 2 && yarn android:install-optimized",
		"android:release": "yarn android:clean && yarn android:bundle-js && yarn android:build-release && yarn android:install-apk",
		"android:generate-aab": "yarn android:clean && yarn android:bundle-js && yarn android:build-aab",
		"android:release-separate": "yarn android:clean && yarn android:bundle-js && yarn android:build-release && yarn android:install-apk",
		"android:initialize": "yarn install && yarn android:onesignal-setup && yarn android:copy-keystore && yarn android:release",
		"ios:xcode": "open ios/HubApp.xcworkspace",
		"ios:clean-build": "cd ios && xcodebuild clean && cd ..",
		"ios:pods-install": "yarn fix-boost && cd ios && pod install && cd ..",
		"ios:pods-clean": "cd ios && rm -rf Pods Podfile.lock && pod deintegrate && pod cache clean --all && cd .. && yarn ios:pods-install --repo-update",
		"ios:clean-derived": "rm -rf ~/Library/Developer/Xcode/DerivedData",
		"ios:bundle-release": "NODE_OPTIONS='--openssl-legacy-provider' npx react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios && echo 'âœ… Bundle gerado! Agora vocÃª pode fazer o archive no Xcode.'",
		"ios:onesignal-setup": "yarn add react-native-onesignal@4.5.4",
		"ios:clean-full": "yarn ios:clean-build && yarn ios:pods-clean",
		"ios:full-reset": "yarn ios:clean-derived && yarn ios:clean-full && yarn ios:xcode",
		"ios:prepare-archive": "yarn ios:clean-build && yarn ios:bundle-release",
		"ios:initialize": "yarn install && yarn ios:pods-install && yarn ios:onesignal-setup && yarn ios:full-reset",
		"reset": "rm -rf node_modules && rm -rf package-lock.json && yarn install",
		"metro:clean": "npx react-native start --reset-cache",
		"watchman:fix": "watchman watch-del . && watchman watch-project ."
	},
	"dependencies": {
		"@expo/match-media": "^0.4.0",
		"@react-native-async-storage/async-storage": "1.15.17",
		"@react-native-community/checkbox": "^0.5.6",
		"@react-native-community/cli": "^4.13.0",
		"@react-native-community/clipboard": "^1.5.1",
		"@react-native-community/datetimepicker": "^3.0.9",
		"@react-native-community/masked-view": "^0.1.10",
		"@react-native-picker/picker": "^1.9.4",
		"@react-navigation/bottom-tabs": "^6.6.1",
		"@react-navigation/native": "^6.1.18",
		"@react-navigation/stack": "^6.4.1",
		"@redux-saga/is": "^1.1.2",
		"axios": "^0.21.0",
		"babel-plugin-module-resolver": "^4.0.0",
		"css-to-react-native": "^3.0.0",
		"jwt-decode": "^3.1.2",
		"microdiff": "1.1.0",
		"moment": "^2.29.1",
		"react": "17.0.2",
		"react-hook-form": "^6.13.1",
		"react-native": "0.67.5",
		"react-native-app-intro-slider": "^4.0.4",
		"react-native-calendars": "^1.1129.0",
		"react-native-datepicker": "^1.7.2",
		"react-native-gesture-handler": "^1.9.0",
		"react-native-image-picker": "3.1.1",
		"react-native-modal": "^11.10.0",
		"react-native-modal-datetime-picker": "^9.1.0",
		"react-native-modalize": "^2.0.8",
		"react-native-onesignal": "4.5.4",
		"react-native-picker-select": "^8.0.4",
		"react-native-popup-menu": "^0.16.1",
		"react-native-reanimated": "^2.17.0",
		"react-native-remote-svg": "^2.0.6",
		"react-native-render-html": "^6.3.4",
		"react-native-responsive-query": "^0.2.2",
		"react-native-safe-area-context": "^4.14.0",
		"react-native-screens": "3.15.0",
		"react-native-share-pdf": "^0.0.7",
		"react-native-simple-radio-button": "^2.7.4",
		"react-native-simple-toast": "^1.1.3",
		"react-native-splash-screen": "^3.2.0",
		"react-native-swipe-list-view": "^3.2.5",
		"react-native-vector-icons": "^7.1.0",
		"react-native-webview": "^11.17.2",
		"react-redux": "^7.2.2",
		"react-responsive": "^9.0.2",
		"redux-saga": "^1.1.3",
		"reduxsauce": "^1.2.0",
		"rn-fetch-blob": "^0.12.0",
		"seamless-immutable": "^7.1.4",
		"styled-components": "^5.2.1",
		"use-debounced-effect": "^0.0.3",
		"vanilla-masker": "^1.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.8.4",
		"@babel/runtime": "^7.8.4",
		"@react-native-community/eslint-config": "^1.1.0",
		"@types/react": "^18.2.8",
		"@types/styled-components": "^5.1.26",
		"babel-jest": "^25.1.0",
		"eslint": "^6.5.1",
		"jest": "^25.1.0",
		"jetifier": "^2.0.0",
		"metro-react-native-babel-preset": "^0.59.0",
		"react-test-renderer": "16.13.1",
		"typescript": "^5.1.3"
	},
	"jest": {
		"preset": "react-native"
	},
	"resolutions": {
		"microdiff": "1.1.0"
	}
}
